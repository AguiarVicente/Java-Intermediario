package com.nil.cursojavaIntermediario.printf;

public class ExemploPrintf {

	public static void main(String[] args) {
		
		System.out.printf("Hello %s\n", "Olá Mundo!");
		
		System.out.printf("%S%n", "Olá mundo!");
		
		System.out.printf("%c", 'c');
		System.out.println();
		System.out.printf("%C", 'c');

	}

}
