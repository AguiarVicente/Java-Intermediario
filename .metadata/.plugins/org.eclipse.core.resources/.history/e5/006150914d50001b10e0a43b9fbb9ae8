package com.nil.cursojavaIntermediario.escopo_variavel;

public class EscopoVariaveis {
	
	private int valor;

	public int getValor() {
		return valor;
	}

	public void setValor(int valor) {
		this.valor = valor;
	}
	
	public int calcularValor(int valor) {
		valor = valor + 10;
		return valor;
	}
	
}
